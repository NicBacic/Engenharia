<head>
<title>Show Users</title>
</head>

<p id="notice"><%= notice %></p>

<p>
  <strong>Account id:</strong>
  <%= @user.id %>
</p>

<p>
  <strong>Username:</strong>
  <%= @user.username %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Hashed password:</strong>
  <%= @user.password_digest %>
</p>

<p>
  <strong>Account created at:</strong>
  <%= @user.created_at %>
</p>

<p>
  <strong>Account last modified at:</strong>
  <%= @user.updated_at %>
</p>

<p>
  <strong>Account activated?:</strong>
  <%= @user.activated %>
</p>

<p>
  <strong>Account activated at:</strong>
  <%= @user.activated_at %>
</p>
